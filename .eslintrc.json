{
  "parser":  "@typescript-eslint/parser",
  "extends": "xo",
  "rules": {
    "no-unused-vars": "off",
    "max-params": [2, 3]
  },
  "globals": {
    "document": false,
    "DOMParser": false,
    "HTMLElement": false,
    "HTMLFormElement": false,
    "window": false,
    "Event": false,
    "XMLHttpRequest": false
  }
}